{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [ "prettier", "eslint:recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
      "no-unused-vars": "off"
  },
  "ignorePatterns": ["dist/*"]
}
